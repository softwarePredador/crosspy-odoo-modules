<?xml version="1.0" encoding="UTF-8" ?>
<odoo>

    <record id="l10n_br_boleto_res_company_form" model="ir.ui.view">
        <field name="name">l10n_br_boleto.res.company.form</field>
        <field name="model">res.company</field>
        <field name="inherit_id" ref="base.view_company_form" />
        <field name="arch" type="xml">
            <xpath expr="//field[@name='currency_id']" position="after">
                <field name="bank_account_id"/>
                <field name="next_bank_number"/>
                <field name="bank_contract"/>
                <field name="bank_transmision_code"/>
                <field name="interest_account_id"/>
            </xpath>
        </field>
    </record>

</odoo>