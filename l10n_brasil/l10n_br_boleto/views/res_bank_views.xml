<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="view_res_bank_slip_form" model="ir.ui.view">
            <field name="name">res.bank.slip.form</field>
            <field name="model">res.bank</field>
            <field name="inherit_id" ref="base.view_res_bank_form" />
            <field name="arch" type="xml">
                <xpath expr="//sheet/group/group[@name='communication_details']" position="inside">
                    <field name="has_slip"/>
                    <field name="logo" widget="image"/>
                </xpath>
            </field>
        </record>
    </data>
</odoo>
 